{
  "name": "vuforia-spatial-edge-server",
  "version": "0.3.1",
  "description": "Vuforia Spatial Edge Server",
  "main": "index.js",
  "dependencies": {
    "archiver": "^3.1.1",
    "body-parser": "^1.19.0",
    "cheerio": "^0.22.0",
    "cors": "^2.8.5",
    "decompress-zip": "^0.3.2",
    "directory-tree": "^2.2.4",
    "express": "^4.17.1",
    "express-handlebars": "^4.0.4",
    "formidable": "^1.2.2",
    "has-ansi": "^4.0.0",
    "ip": "^1.1.5",
    "jimp": "^0.10.3",
    "lodash": "^4.17.15",
    "module-alias": "^2.2.2",
    "monaco-editor": "^0.20.0",
    "network-interfaces": "^1.1.0",
    "node-fetch": "^2.6.0",
    "node-persist": "^2.1.0",
    "request": "^2.*",
    "simple-git": "^2.5.0",
    "socket.io": "~2.2.0",
    "socket.io-client": "~2.3.0",
    "winston": "^3.2.1",
    "xml2js": "^0.4.23",
    "wedoboostpoweredup": "^2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ptcrealitylab/vuforia-spatial-edge-server"
  },
  "devDependencies": {
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "electron-packager": "^14.2.1",
    "eslint": "^6.8.0",
    "jsdoc": "^3.6.4",
    "minami": "^1.2.3"
  },
  "build": {
    "appId": "com.electron.vstedgeserver",
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  },
  "scripts": {
    "test": "npm run lint && NODE_ENV=test node test.js",
    "test-node-six-mobile": "NODE_ENV=test FORCE_MOBILE=true node test.js",
    "lint": "eslint .",
    "generate-docs": "jsdoc -c .jsdoc.conf -r .",
    "start": "electron index.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Vuforia Spatial Edge Server\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "_moduleAliases": {
    "@libraries": "./libraries"
  },
  "author": {
